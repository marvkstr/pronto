    {
      "@id": "http://lexvo.org/ontology#label",
      "@type": ["http://www.w3.org/2002/07/owl#ObjectProperty"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "If C lvont:Label L and L skosxl:literalForm V, then X rdfs:label V.",
          "@language": "en"
        },
        {
          "@value": "the semiotic property of possessing a natural language label (as opposed to artificial identifiers), e.g. the city of Paris is lexicalized in Italian using the Italian term \"Parigi\".",
          "@language": "en"
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [
        { "@value": "has label", "@language": "en" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#range": [
        { "@id": "http://www.w3.org/2008/05/skos-xl#Label" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#subPropertyOf": [
        { "@id": "http://lexvo.org/ontology#representedBy" }
      ],
      "http://www.w3.org/2002/07/owl#inverseOf": [
        { "@id": "http://lexvo.org/ontology#means" }
      ],
      "http://www.w3.org/2004/02/skos/core#note": [
        {
          "@value": "This property is more general than skosxl:prefLabel and skosxl:altLabel, as it does not make any normative claims about which labels are preferred. It merely describes\n    the fact that a certain label can be used to refer to something.",
          "@language": "en"
        }
      ]
    },
