      "@id": "http://www.w3.org/2004/02/skos/core#exactMatch",
      "http://www.w3.org/2000/01/rdf-schema#subPropertyOf": [
        { "@id": "http://lexvo.org/ontology#nearlySameAs" }
      ]
    },
    {
