      "http://www.w3.org/2000/01/rdf-schema#range": [
        { "@id": "http://www.w3.org/2001/XMLSchema#nonNegativeInteger" }
      ],
      "http://www.w3.org/2004/02/skos/core#note": [
        { "@value": "Especially for CJK characters.", "@language": "en" }
