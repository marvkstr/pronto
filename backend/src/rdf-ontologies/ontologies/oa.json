    },
    {
      "@id": "http://www.w3.org/ns/oa#",
      "http://purl.org/dc/elements/1.1/title": [{ "@value": "Web Annotation Ontology" }],
      "http://purl.org/dc/terms/creator": [
        { "@id": "_:c14n0" },
        { "@id": "_:c14n1" },
        { "@id": "_:c14n2" }
      ],
      "http://purl.org/dc/terms/modified": [{ "@value": "2016-11-12T21:28:11Z" }],
      "@type": ["http://www.w3.org/2002/07/owl#Ontology"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The Web Annotation ontology defines the terms of the Web Annotation vocabulary. Any changes to this document MUST be from a Working Group in the W3C that has established expertise in the area."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#seeAlso": [
        { "@id": "http://www.w3.org/TR/annotation-vocab/" }
      ],
      "http://www.w3.org/2002/07/owl#versionInfo": [{ "@value": "2016-11-12T21:28:11Z" }],
      "http://www.w3.org/ns/prov#wasRevisionOf": [
        { "@id": "http://www.openannotation.org/spec/core/20130208/oa.owl" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#Annotation",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        { "@value": "The class for Web Annotations." }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "Annotation" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#Choice",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A subClass of  as:OrderedCollection  that conveys to a consuming application that it should select one of the resources in the  as:items  list to use, rather than all of them.  This is typically used to provide a choice of resources to render to the user, based on further supplied properties.  If the consuming application cannot determine the user's preference, then it should use the first in the list."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "Choice" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/activitystreams#OrderedCollection" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#CssSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A CssSelector describes a Segment of interest in a representation that conforms to the Document Object Model through the use of the CSS selector specification."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "CssSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#CssStyle",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A resource which describes styles for resources participating in the Annotation using CSS."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "CssStyle" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Style" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#DataPositionSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "DataPositionSelector describes a range of data by recording the start and end positions of the selection in the stream. Position 0 would be immediately before the first byte, position 1 would be immediately before the second byte, and so on. The start byte is thus included in the list, but the end byte is not."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "DataPositionSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#Direction",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A class to encapsulate the different text directions that a textual resource might take.  It is not used directly in the Annotation Model, only its three instances."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "Direction" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#FragmentSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The FragmentSelector class is used to record the segment of a representation using the IRI fragment specification defined by the representation's media type."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "FragmentSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#HttpRequestState",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The HttpRequestState class is used to record the HTTP request headers that a client SHOULD use to request the correct representation from the resource. "
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "HttpRequestState" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#State" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#Motivation",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The Motivation class is used to record the user's intent or motivation for the creation of the Annotation, or the inclusion of the body or target, that it is associated with."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "Motivation" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/2004/02/skos/core#Concept" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#PreferContainedDescriptions",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Resource"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "An IRI to signal the client prefers to receive full descriptions of the Annotations from a container, not just their IRIs."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [
        { "@value": "PreferContainedDescriptions" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#PreferContainedIRIs",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Resource"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "An IRI to signal that the client prefers to receive only the IRIs of the Annotations from a container, not their full descriptions."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "PreferContainedIRIs" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#RangeSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A Range Selector can be used to identify the beginning and the end of the selection by using other Selectors. The selection consists of everything from the beginning of the starting selector through to the beginning of the ending selector, but not including it."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "RangeSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#ResourceSelection",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "Instances of the ResourceSelection class identify part (described by an oa:Selector) of another resource (referenced with oa:hasSource), possibly from a particular representation of a resource (described by an oa:State). Please note that ResourceSelection is not used directly in the Web Annotation model, but is provided as a separate class for further application profiles to use, separate from oa:SpecificResource which has many Annotation specific features."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "ResourceSelection" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#Selector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A resource which describes the segment of interest in a representation of a Source resource, indicated with oa:hasSelector from the Specific Resource. This class is not used directly in the Annotation model, only its subclasses."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "Selector" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#SpecificResource",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "Instances of the SpecificResource class identify part of another resource (referenced with oa:hasSource), a particular representation of a resource, a resource with styling hints for renders, or any combination of these, as used within an Annotation."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "SpecificResource" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#ResourceSelection" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#State",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A State describes the intended state of a resource as applied to the particular Annotation, and thus provides the information needed to retrieve the correct representation of that resource."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "State" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#Style",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A Style describes the intended styling of a resource as applied to the particular Annotation, and thus provides the information to ensure that rendering is consistent across implementations."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "Style" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#SvgSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "An SvgSelector defines an area through the use of the Scalable Vector Graphics [SVG] standard. This allows the user to select a non-rectangular area of the content, such as a circle or polygon by describing the region using SVG. The SVG may be either embedded within the Annotation or referenced as an External Resource."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "SvgSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#TextPositionSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The TextPositionSelector describes a range of text by recording the start and end positions of the selection in the stream. Position 0 would be immediately before the first character, position 1 would be immediately before the second character, and so on."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "TextPositionSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#TextQuoteSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The TextQuoteSelector describes a range of text by copying it, and including some of the text immediately before (a prefix) and after (a suffix) it to distinguish between multiple copies of the same sequence of characters."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "TextQuoteSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#TextualBody",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [{ "@value": "" }],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "TextualBody" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#TimeState",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A TimeState records the time at which the resource's state is appropriate for the Annotation, typically the time that the Annotation was created and/or a link to a persistent copy of the current version."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "TimeState" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#State" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#XPathSelector",
      "@type": ["http://www.w3.org/2000/01/rdf-schema#Class"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": " An XPathSelector is used to select elements and content within a resource that supports the Document Object Model via a specified XPath value."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "XPathSelector" }],
      "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
        { "@id": "http://www.w3.org/ns/oa#Selector" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#annotationService",
      "@type": ["http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The object of the relationship is the end point of a service that conforms to the annotation-protocol, and it may be associated with any resource.  The expectation of asserting the relationship is that the object is the preferred service for maintaining annotations about the subject resource, according to the publisher of the relationship.\n\n  This relationship is intended to be used both within Linked Data descriptions and as the  rel  type of a Link, via HTTP Link Headers rfc5988 for binary resources and in HTML <link> elements.  For more information about these, please see the Annotation Protocol specification annotation-protocol.\n  "
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "annotationService" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#assessing",
      "@type": ["http://www.w3.org/ns/oa#Motivation"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The motivation for when the user intends to provide an assessment about the Target resource."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "assessing" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#bodyValue",
      "@type": ["http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The object of the predicate is a plain text string to be used as the content of the body of the Annotation.  The value MUST be an  xsd:string  and that data type MUST NOT be expressed in the serialization. Note that language MUST NOT be associated with the value either as a language tag, as that is only available for  rdf:langString .\n  "
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#domain": [
        { "@id": "http://www.w3.org/ns/oa#Annotation" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "bodyValue" }],
      "http://www.w3.org/2000/01/rdf-schema#range": [
        { "@id": "http://www.w3.org/2001/XMLSchema#string" }
      ]
    },
    {
      "@id": "http://www.w3.org/ns/oa#bookmarking",
      "@type": ["http://www.w3.org/ns/oa#Motivation"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "The motivation for when the user intends to create a bookmark to the Target or part thereof."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#isDefinedBy": [
        { "@id": "http://www.w3.org/ns/oa#" }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "bookmarking" }]
    },
    {
      "@id": "http://www.w3.org/ns/oa#cachedSource",
      "@type": ["http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"],
      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "A object of the relationship is a copy of the Source resource's representation, appropriate for the Annotation."
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#domain": [
        { "@id": "http://www.w3.org/ns/oa#TimeState" }
      ],
