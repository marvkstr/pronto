      "http://www.w3.org/2000/01/rdf-schema#comment": [
        {
          "@value": "relates a namespace to a transformation for\n    all documents in that namespace"
        }
      ],
      "http://www.w3.org/2000/01/rdf-schema#label": [{ "@value": "namespaceTransformation" }],
      "http://www.w3.org/2000/01/rdf-schema#range": [
